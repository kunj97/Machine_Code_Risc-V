addi x5,x0,5   	#Assignment 1
addi x6,x0,5		#Assignment 2
addi x7,x0,10   #Assignment 3
addi x8,x0,15		#Mid-term 
addi x9,x0,15		#Project
addi x10,x0,50  #Exam 

beq x5,x0,L1

beq x6,x0,L1

beq x8,x0,L2  

L1:

addi x11,x0,10
add  x11,x11,x7
add  x11,x11,x8
add  x11,x11,x9
add  x11,x11,x10
beq x0,x0, exit

L2:

addi x11,x0,60
add x11,x11,x5
add x11,x11,x6
add x11,x11,x7
add x11,x11,x9
beq x0,x0,exit 

exit:

